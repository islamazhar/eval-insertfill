#maximal read length
max_rd_len=100
[LIB]
#average insert size
avg_ins=150
#if sequence needs to be reversed
reverse_seq=0
#in which part(s) the reads are used
asm_flags=4
#use only first 100 bps of each read
rd_len_cutoff=100
#in which order the reads are used while scaffolding
rank=1
#a pair of fastq file, read 1 file should always be followed by read 2 file
q1=/cs/work/scratch/riqwalve/eval-insertfill/data/reads0_pe1.fq
q2=/cs/work/scratch/riqwalve/eval-insertfill/data/reads0_pe2.fq
[LIB]
avg_ins=1500
reverse_seq=0
asm_flags=4
rank=2
# cutoff of pair number for a reliable connection (at least 5 for large insert size)
pair_num_cutoff=5
# #minimum aligned length to contigs for a reliable read location (at least 35 for large insert size)
map_len=35
q1=/cs/work/scratch/riqwalve/eval-insertfill/data/reads1_pe1.fq
q2=/cs/work/scratch/riqwalve/eval-insertfill/data/reads1_pe2.fq
[LIB]
avg_ins=3000
reverse_seq=0
asm_flags=4
rank=3
pair_num_cutoff=5
map_len=35
q1=/cs/work/scratch/riqwalve/eval-insertfill/data/reads2_pe1.fq
q2=/cs/work/scratch/riqwalve/eval-insertfill/data/reads2_pe2.fq
